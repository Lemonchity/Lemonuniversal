]]
-- Load the UI library
local Library = loadstring(game:HttpGet('https://gist.githubusercontent.com/MjContiga1/5b9535166d60560ac884a871cb0dc418/raw/e7fdb16802d9486d8d04d3e41d3607d89e6b4a1b/Libsuck.lua'))()

-- Create main window
local window = Library:Window('Example UI')

-- Create tabs with icons replace ur own icon id
local mainTab = window:Tab({"Crafting", "rbxassetid://7734022041"})
local localTab = window:Tab({"LocalPlayer", "rbxassetid://7743875962"})
local settingsTab = window:Tab({"Reward", "rbxassetid://7733673987"})

-- Main Tab Elements
mainTab:Label("Welcome to the UI Library!")

-- Button
localTab:Button('Jump high50 risky', function()      
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local UIS = game:GetService("UserInputService")

local HIGH_JUMP_POWER = 150
local NORMAL_JUMP_POWER = 50
local isHighJumpEnabled = false

local function setJumpPower(character, power)
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        humanoid.JumpPower = power
    end
end

local function toggleHighJump()
    isHighJumpEnabled = not isHighJumpEnabled
    local character = player.Character
    if character then
        if isHighJumpEnabled then
            setJumpPower(character, HIGH_JUMP_POWER)
            print("Высокий прыжок ВКЛЮЧЕН")
        else
            setJumpPower(character, NORMAL_JUMP_POWER)
            print("Высокий прыжок ВЫКЛЮЧЕН")
        end
    end
end

-- Для мобильных устройств, в отсутствии клавиш, используем экранные кнопки через TouchTap
-- Пример простой toggle с использованием двойного нажатия по экрану:

local lastTap = 0
local TAP_DELAY = 0.5 -- секунды между тапами

UIS.TouchTap:Connect(function()
    local currentTime = tick()
    if currentTime - lastTap <= TAP_DELAY then
        toggleHighJump()
        lastTap = 0
    else
        lastTap = currentTime
    end
end)

-- Автоматическое применение при смене персонажа

player.CharacterAdded:Connect(function(character)
    if isHighJumpEnabled then
        setJumpPower(character, HIGH_JUMP_POWER)
    else
        setJumpPower(character, NORMAL_JUMP_POWER)
    end
end)

-- Инициализация при старте

if player.Character then
    setJumpPower(player.Character, NORMAL_JUMP_POWER)
end
    print("Activating")
    game.StarterGui:SetCore("SendNotification", {
        Title = "NOTIFY";
        Text = "TO ENABLE/DISABLE DOUBLE TAP ON SCREEN";
        Duration = 7;
    })
end)

-- Button
localTab:Button('Walkspeed50 risky', function()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
humanoid.WalkSpeed = 50
    print("Activating")
    game.StarterGui:SetCore("SendNotification", {
        Title = "Script Made By";
        Text = "LemonHubOffical enjoy!";
        Duration = 9;
    })
end)
